  "homepage": "https://github.com/benbjurstrom/curator-model",
  "authors": [
    {
      "name": "Ben Bjurstrom",
      "email": "ben@jelled.com"
    }
  ],
  "keywords": [
    "curator",
    "twitter api",
    "eloquent model"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.6.4"
  },
  "require-dev": {
